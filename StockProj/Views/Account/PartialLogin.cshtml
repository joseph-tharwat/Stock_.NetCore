@model AccountModel

<div class= "partial-view">
    <div class="form-group">
        <label>username</label>
        @Html.TextBoxFor(account => account.UserName, new { @class = "form-control"})
    </div>
    <div class="form-group">
        <label>password</label>
        @Html.PasswordFor(account => account.Password, new { @class = "form-control"})
    </div>
</div>
